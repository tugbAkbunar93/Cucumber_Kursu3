package StepDefinitions;

import io.cucumber.java.en.*; // given then when hepsini görmesi için * koydum

public class _01_LoginSteps {

    @Given("Navigate to basqar")
    public void navigate_to_basqar() {
        System.out.println("Merhaba Cucumber1");
    }
    @When("Enter username and password and click login button")
    public void enter_username_and_password_and_click_login_button() {
        System.out.println("Merhaba Gherkin1");
    }
    @Then("User should login successfuly")
    public void user_should_login_successfuly() {
        System.out.println("Merhaba Maven1");
    }

}
